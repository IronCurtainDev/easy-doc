{
    "name": "ironcurtaindev/easy-doc",
    "description": "A lightweight Laravel package for API documentation generation with PHP 8 Attributes support and configurable headers",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "documentation",
        "swagger",
        "openapi",
        "postman",
        "php-attributes",
        "sdk"
    ],
    "authors": [
        {
            "name": "Easy Pack",
            "email": "support@easypack.dev"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "symfony/yaml": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "EasyDoc\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/_ide_helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "EasyDoc\\EasyDocServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}